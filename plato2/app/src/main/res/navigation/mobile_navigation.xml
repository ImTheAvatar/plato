<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.elham.plato.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_game"
        android:name="com.elham.plato.ui.game.GameFragment"
        android:label="Game"
        tools:layout="@layout/fragment_game" />


    <fragment
        android:id="@+id/navigation_friends"
        android:name="com.elham.plato.ui.friends.FriendsFragment"
        android:label="Friends"
        tools:layout="@layout/fragment_friends" />


    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.elham.plato.ui.chat.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_shop"
        android:name="com.elham.plato.ui.shop.ShopFragment"
        android:label="Shop"
        tools:layout="@layout/fragment_shop" />
</navigation>
